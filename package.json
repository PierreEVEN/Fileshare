{
  "name": "evenpierre",
  "version": "0.0.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "debug": "npm-run-all --parallel debug:build dev:start",
    "debug:build": "webpack --watch --config webpack.debug.js",
    "dev": "npm-run-all --parallel dev:build dev:start",
    "dev:build": "webpack --watch --config webpack.dev.js",
    "dev:start": "nodemon --ignore public bin/www",
    "prod": "npm-run-all --parallel rel:build rel:start",
    "rel:build": "webpack --config webpack.prod.js",
    "rel:start": "node bin/www",
    "run_production": "prod",
    "pm2:start": "pm2 start bin/www"
  },
  "dependencies": {
    "@babel/core": "^7.22.20",
    "@babel/preset-env": "^7.22.20",
    "archiver": "^6.0.0",
