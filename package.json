    "babel-plugin-prismjs": "^2.1.0",
    "bcrypt": "^5.1.0",
    "css-loader": "^6.8.1",
    "debug": "~2.6.9",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "filecompare": "^1.0.4",
    "fluent-ffmpeg": "^2.1.2",
    "gm": "^1.25.0",
    "handlebars": "^4.7.8",
    "http-errors": "~1.6.3",
    "mime": "^3.0.0",
    "mini-css-extract-plugin": "^2.7.6",
    "node-sass": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "pdfjs-dist": "^3.10.111",
    "pg": "^8.11.3",
    "prism": "^1.0.0",
    "prismjs": "^1.29.0",
    "pug": "^3.0.2",
    "sass-loader": "^13.3.2",
    "sharp": "^0.32.5",
    "showdown": "^2.1.0",
    "style-loader": "^3.3.3",
    "video.js": "^8.5.2",
    "webpack": "^5.88.2",
    "webpack-bundle-analyzer": "^4.9.1",
    "webpack-cli": "^5.1.4",
    "webpack-merge": "^5.9.0",
    "winston": "^3.10.0"
  }
}
