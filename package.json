{
  "name": "evenpierre",
  "version": "0.0.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "dev": "npm-run-all --parallel dev:build dev:start ",
    "dev:build": "webpack --watch --config webpack.dev.js",
    "dev:start": "nodemon --ignore public/* ./bin/www",
    "prod": "npm-run-all rel:build rel:start",
    "rel:build": "webpack --config webpack.prod.js",
    "rel:start": "node ./bin/www",
    "run_production": "prod",
    "pm2:start": "pm2 start ./bin/www"
  },
  "dependencies": {
    "archiver": "^6.0.0",
    "bcrypt": "^5.1.0",
    "css-loader": "^6.8.1",
    "debug": "~2.6.9",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "filecompare": "^1.0.4",
    "fluent-ffmpeg": "^2.1.2",
    "handlebars": "^4.7.8",
    "http-errors": "~1.6.3",
    "mariadb": "^3.2.0",
    "mime": "^3.0.0",
    "mini-css-extract-plugin": "^2.7.6",
    "node-sass": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "pug": "^3.0.2",
    "sass-loader": "^13.3.2",
    "sharp": "^0.32.5",
    "style-loader": "^3.3.3",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-merge": "^5.9.0",
    "winston": "^3.10.0"
  }
}
